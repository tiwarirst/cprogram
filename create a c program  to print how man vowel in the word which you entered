#include<stdio.h>
#include<string.h>
int main()
{
    char s[200],d;
    printf("enter the string:");
    scanf("%[^\n]%*c",s);
    int c=0;
    for(int i=0;s[i]!='\0';i++)
    {
        if(s[i]=='a'||s[i]=='e'||s[i]=='i'||s[i]=='o'||s[i]=='u')
        c++;
    }
    printf("%d",c);
    return 0;
}
