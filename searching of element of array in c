#include <stdio.h>
int main()
{
    int n,d;
    printf("enter the size of array");
    scanf("%d",&n);
    int a[n];
    for(int i=0;i<n;i++)
    {printf("enter the %d element of array",i);
    scanf("%d",&a[i]);}
    printf("enter the elemnt to search");
    scanf("%d",&d);
    for (int j=0;j<n;j++)
    {if(a[j]==d)
    printf("%d",j);}
    return 0;
    }
